	Journal = {J. Neurosci.},
	Keywords = {TEMPORAL RECEPTIVE WINDOWS; INFERIOR PARIETAL CORTEX; FREE-ENERGY PRINCIPLE; VENTRAL STREAM; VISUAL-CORTEX; COMPREHENSION; LANGUAGE; BRAIN; SPEECH; SENTENCE},
