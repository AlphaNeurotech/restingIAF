	Issn = {0010-9452},
	Journal = {Cortex},
	Keywords = {NUCLEUS-ACCUMBENS DOPAMINE; DECISION-MAKING; HYPOTHETICAL REWARDS; BASAL GANGLIA; MOTIVATION; APATHY; REAL; DELAY; VIGOR; MONEY},
	Month = {August},
