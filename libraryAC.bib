	Issn = {0270-6474},
	Journal = {J. Neurosci.},
	Keywords = {SOURCE-DENSITY ANALYSIS; FIELD POTENTIALS; AWAKE MACAQUE; ATTENTIONAL MODULATION; NEURONAL OSCILLATIONS; SELECTIVE ATTENTION; MEMORY PERFORMANCE; EEG ALPHA; MU RHYTHM; CORTEX},
