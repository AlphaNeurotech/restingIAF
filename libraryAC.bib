	Journal = {J. Neurosci.},
	Keywords = {VISUAL-CORTEX V1; NEURONAL SYNCHRONIZATION; NEUROMAGNETIC RESPONSES; SPATIAL ATTENTION; EVOKED-RESPONSES; BRAIN ACTIVITY; HUMANS; OSCILLATIONS; AWARENESS; CORTICES},
