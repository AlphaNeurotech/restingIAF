	Date-Added = {2017-02-20 12:21:28 +0000},
	Date-Modified = {2017-02-20 12:21:28 +0000},
	Doi = {10.1016/j.neuropsychologia.2014.01.007},
	Issn = {0028-3932},
	Journal = {Neuropsychologia},
	Keywords = {EVENT-RELATED POTENTIALS; LANGUAGE COMPREHENSION; BRAIN POTENTIALS; NEURAL MECHANISMS; MOSES ILLUSION; ERP; SENTENCE; MEMORY; N400; OSCILLATIONS},
	Month = {April},
