	Journal = {J. Neurosci.},
	Keywords = {VISUAL COMPETITION; PERCEPTION; CORTEX; MOTION; ATTENTION; TOUCH; COHERENCE; STIMULI; ENTRAINMENT; VOLUNTARY},
	Month = {January},
	Number = {3},
