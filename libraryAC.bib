	Doi = {10.1016/j.cub.2014.11.034},
	Issn = {0960-9822},
	Journal = {Curr. Biol.},
	Keywords = {TRANSCRANIAL MAGNETIC STIMULATION; INHIBITION-TIMING HYPOTHESIS; INDUCED VISUAL ILLUSION; INDUCED FLASH ILLUSION; MULTISENSORY INTERACTIONS; HUMAN V1; SOUND; CORTEX; TIME; SPACE},
	Month = {January},
