	Keywords = {ANTERIOR CINGULATE CORTEX; DORSOLATERAL PREFRONTAL CORTEX; DECISION-MAKING; ORBITOFRONTAL CORTEX; BASOLATERAL AMYGDALA; COGNITIVE EFFORT; PARIETAL CORTEX; PHYSICAL EFFORT; INDIVIDUAL-DIFFERENCES; NEURAL MECHANISMS},
	Month = {February},
