	Keywords = {CLASSICAL RECEPTIVE-FIELD; TEMPORAL VISUAL-CORTEX; MISMATCH NEGATIVITY; BAYESIAN-INFERENCE; SINGLE NEURONS; MACAQUE MONKEY; AREA MT; CONNECTIONS; INFORMATION; BRAIN},
