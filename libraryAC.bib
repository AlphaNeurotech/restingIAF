	Keywords = {LANGUAGE COMPREHENSION; OLDER-ADULTS; ALPHA-FREQUENCY; EEG ALPHA; BRAIN; ERP; OSCILLATIONS; POTENTIALS; HYPOTHESIS; YOUNGER},
	Month = {November},
