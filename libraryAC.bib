	Journal = {Arch. Ital. Biol.},
	Keywords = {BRAIN; NETWORKS; CONNECTIONS; TRANSIENTS; MACHINES; DYNAMICS},
	Month = {December},
	Number = {4},
	Pages = {290--326},
	Publisher = {UNIV PISA},
	Times-Cited = {12},
